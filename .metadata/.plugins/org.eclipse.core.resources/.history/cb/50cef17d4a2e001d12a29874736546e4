#include <stdio.h>
#include <stdlib.h>

int perdirNumero (void){
	setbuf(stdout, NULL);
	int numeroIngresado;
	int retorno;

		printf ("Ingrese un numero:\n");
		scanf("%d",&numeroIngresado);

	retorno = numeroIngresado;

	return retorno;
}// fin de pedirNumero


