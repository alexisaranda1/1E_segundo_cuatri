//bibliotecas.
#include <stdio.h>
#include <stdlib.h>
int sumarEnteros(int numeroUno, int numeroDos);// prototipo firma de la fiÂ¿uncion
// int main funcion principal.
//nombre parametros y debolucion.

int main(void) {


	//variables.
	int numeroUno;
	int numeroDos;
	int suma;

	//pedir un numro por consola por scanf.
	printf("numeroUno: ");
	scanf("%d", &numeroUno); // "%d" de digito.
	printf("numeroDos: ");
	scanf("%d", &numeroDos);
	fflush(stdin);

	suma = sumarEnteros(numeroUno, numeroDos);

	printf(" La suma es: %d ", suma);

	return 0;
}

// defino una funcion, le doy los parametros

int sumarEnteros(int numeroUno, int numeroDos) {

	int suma;

	suma = numeroUno + numeroDos;

	return suma; // retorno el resulado
}

